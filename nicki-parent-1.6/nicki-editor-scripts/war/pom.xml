<?xml version="1.0" encoding="UTF-8"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>nicki-parent</artifactId>
		<groupId>org.mgnl.nicki</groupId>
		<version>1.1</version>
	</parent>
	<artifactId>nicki-editor-scripts.war</artifactId>
	<version>1.0-SNAPSHOT</version>
	<name>nicki-editor-scripts.war</name>
	<packaging>war</packaging>
	<dependencies>
		<dependency>
			<artifactId>nicki-editor-scripts</artifactId>
			<groupId>org.mgnl.nicki</groupId>
			<version>1.1</version>
		</dependency>
		<dependency>
			<artifactId>nicki-novell-userapp</artifactId>
			<groupId>org.mgnl.nicki</groupId>
			<version>1.2</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
					<includes>
						<include>*NOTHING*</include>
					</includes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.1</version>
				<configuration>
					<webXml>../WebContent/WEB-INF/web.xml</webXml>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
