version: 1

#NDS attribute:displayName
#Syntax:SYN_CI_STRING
dn: cn=schema
changetype: modify
add: attributeTypes
attributeTypes: (
  2.16.840.1.113730.3.1.241
  NAME 'displayName'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  )

#NDS attribute:nickiRoleRef
#Syntax:SYN_DIST_NAME
dn: cn=schema
changetype: modify
add: attributeTypes
attributeTypes: (
  nickiroleref-oid
  NAME 'nickiRoleRef'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.12
  SINGLE-VALUE
  X-NDS_NOT_SCHED_SYNC_IMMEDIATE '1'
  )

