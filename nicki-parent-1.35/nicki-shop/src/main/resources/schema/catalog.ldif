#NDS class:nickiCatalog
dn: cn=schema
changetype: modify
add: objectClasses
objectClasses: (
  nickicatalog-oid
  NAME 'nickiCatalog'
  STRUCTURAL
  SUP ( Top )
  X-NDS_CONTAINMENT 'organizationalUnit'
  X-NDS_NAMING 'cn'
  MUST ( cn )
  MAY ( nickiCategory )
  )

#NDS class:nickiCatalogPage
dn: cn=schema
changetype: modify
add: objectClasses
objectClasses: (
  nickicatalogpage-oid
  NAME 'nickiCatalogPage'
  STRUCTURAL
  SUP ( Top )
  X-NDS_CONTAINMENT ( 'nickiCatalog' 'nickiCatalogPage' )
  X-NDS_NAMING 'cn'
  MUST ( cn )
  MAY ( nickiAttributes $ nickiCategory )
  )

#NDS class:nickiCatalogArticle
dn: cn=schema
changetype: modify
add: objectClasses
objectClasses: (
  nickicatalogarticle-oid
  NAME 'nickiCatalogArticle'
  STRUCTURAL
  SUP ( Top )
  X-NDS_CONTAINMENT 'nickiCatalogPage'
  X-NDS_NAMING 'cn'
  MUST ( cn )
  MAY ( displayName $ nickiAttributes $ nickiCategory $ nickiDescription $ nickiResourceRef $ 
   nickiRoleRef )
  X-NDS_NOT_CONTAINER '1'
  )

