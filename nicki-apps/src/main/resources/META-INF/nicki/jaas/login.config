dev {
	org.mgnl.nicki.core.auth.NickiAdapterLoginModule sufficient adapter=org.mgnl.nicki.core.auth.DevSSOAdapter debug=true;
};

kerberos {
	org.mgnl.nicki.core.auth.NickiAdapterLoginModule sufficient adapter=org.mgnl.nicki.portlets.IframeAdapter debug=false;	
	org.mgnl.nicki.core.auth.SimpleKerberosLoginModule sufficient debug=false;	
};

pnw {
	org.mgnl.nicki.core.auth.NickiAdapterLoginModule sufficient adapter=org.mgnl.nicki.portlets.IframeAdapter debug=false;	
	org.mgnl.nicki.core.auth.SimpleKerberosLoginModule sufficient debug=false;	
};

dam {
	org.mgnl.nicki.core.auth.NickiAdapterLoginModule sufficient adapter=org.mgnl.nicki.core.auth.BasicAuthAdapter debug=false;	
};

dam_dev {
	org.mgnl.nicki.core.auth.NickiAdapterLoginModule sufficient adapter=org.mgnl.nicki.core.auth.BasicAuthAdapter debug=false;	
	org.mgnl.nicki.core.auth.NickiAdapterLoginModule sufficient adapter=org.mgnl.nicki.core.auth.DevSSOAdapter debug=true;
};

spnego-server {
	com.sun.security.auth.module.Krb5LoginModule required
	storeKey=true
	isInitiator=false;
};

spnego-client {
	com.sun.security.auth.module.Krb5LoginModule required;
};